<?xml version="1.0" encoding="UTF-8"?>
<application>
  <id type="desktop">gnome-internet-radio-locator.desktop</id>
  <name>GNOME Internet Radio Locator</name>
  <summary>Listen to and record from Internet radio</summary>
  <licence>CC0</licence>
  <description>
    <_p>
      GNOME Internet Radio Locator allows users to easily find and
      listen to live radio programs on radio broadcasters on the
      Internet.
    </_p>
    <_p>
      GNOME Internet Radio Locator is developed for the GNOME desktop and requires gstreamer 1.0
      to be installed for playback of supported radio stations.
    </_p>
    <_p>
      Enjoy listening to and recording from supported Internet Radio stations in GNOME Internet Radio Locator.
    </_p>
  </description>
  <url type="homepage">https://wiki.gnome.org/Apps/GNOMEInternetRadioLocator</url>
  <screenshots>
    <screenshot type="default">https://git.gnome.org/browse/gnome-internet-radio-locator/plain/data/screenshot.png</screenshot>
  </screenshots>
  <updatecontact>gnome-internet-radio-locator-list@gnome.org</updatecontact>
</application>
